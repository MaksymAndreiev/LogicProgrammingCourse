% Факти
speciality('John Smith', 'Engineer').
speciality('Alice Johnson', 'Teacher').
speciality('David Brown', 'Lawyer').
speciality('Emily Davis', 'Doctor').
speciality('James Taylor', 'Writer').
speciality('Sarah Wilson', 'Designer').
speciality('Michael Miller', 'Programmer').
speciality('Laura Lee', 'Artist').
speciality('William Clark', 'Scientist').
speciality('Elizabeth Scott', 'Musician').

% Зовнішня мета
profession_is :-
    write('Name? '),
    read(Name),
    speciality(Name, Profession),
    write(Profession),
    nl,
    fail.

